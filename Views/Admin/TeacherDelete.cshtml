@model MyWebApp.Models.Teacher

@{
    ViewData["Title"] = "Teacher Delete";
}


<h1 class="textstylei">Coach's Information Delete</h1>

<div style="margin-top: 40px;">
    <h3 style="font-weight:bold;color: rgb(105, 152, 240);">Are you sure you want to delete this?</h3>
    <div style="margin-top: 30px;">
        <hr />

        <dl class="row">
           <dt class="col-sm-2 uplabel">
                Name
            </dt>
            <dd class="col-sm-10 info">
                : @Html.DisplayFor(model => model.Name)
            </dd>
          <dt class="col-sm-2 uplabel">
                @Html.DisplayNameFor(model => model.FullName)
            </dt>
            <dd class="col-sm-10 info">
                : @Html.DisplayFor(model => model.FullName)
            </dd>
           <dt class="col-sm-2 uplabel">
                @Html.DisplayNameFor(model => model.Email)
            </dt>
            <dd class="col-sm-10  info">
                : @Html.DisplayFor(model => model.Email)
            </dd>
           <dt class="col-sm-2 uplabel">
                @Html.DisplayNameFor(model => model.PhoneNUmber)
            </dt>
            <dd class="col-sm-10 info">
                : @Html.DisplayFor(model => model.PhoneNUmber)
            </dd>
        </dl>
        <form enctype="multipart/form-data" method="post" asp-action="TeacherDelete">

            <!-- form enctype="multipart/form-data" method ="post" asp-action="TeacherDelete" -->
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Delete" class="btn2 btn2up" /> 

        </form>
    </div>
</div>



@section Scripts {
@{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
