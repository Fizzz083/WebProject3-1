@model MyWebApp.Models.Archive

@{
    ViewData["Title"] = "Archive Delete";
}


<h1 class="textstylei">Archive Delete</h1>

<div style="margin-top: 40px;">
    <h3 style="font-weight:bold;color: rgb(105, 152, 240);">Are you sure you want to delete this?</h3>
    <div style="margin-top: 30px;">
        <hr />
        <dl class="row">
            <dt class="col-sm-2 uplabel">
                @Html.DisplayNameFor(model => model.Year)
            </dt>
            <dd class="col-sm-10 info" >
                : @Html.DisplayFor(model => model.Year)
            </dd>
            <dt class="col-sm-2 uplabel">
                @Html.DisplayNameFor(model => model.ContestName)
            </dt>
            <dd class="col-sm-10 info">
                : @Html.DisplayFor(model => model.ContestName)
            </dd>
          <dt class="col-sm-2 uplabel">
                @Html.DisplayNameFor(model => model.TeamName)
            </dt>
            <dd class="col-sm-10 info" >
                : @Html.DisplayFor(model => model.TeamName)
            </dd>
            <dt class="col-sm-2 uplabel">
                @Html.DisplayNameFor(model => model.Position)
            </dt>
            <dd class="col-sm-10 info" >
                : @Html.DisplayFor(model => model.Position)
            </dd>

        </dl>

        <form asp-action="ArchiveDelete">
            <input type="hidden" asp-for="AId" />
            <input type="submit" value="Delete" class="btn2 btn2up" /> 
        </form>
    </div>
</div>
