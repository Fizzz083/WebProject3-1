@model MyWebApp.Models.Team

@{
    ViewData["Title"] = "Team Delete";
}


<h1 class="textstylei">Team Delete</h1>

<div style="margin-top: 40px;">
    <h3 style="font-weight:bold;color: rgb(105, 152, 240);">Are you sure you want to delete this?</h3>
    <div style="margin-top: 30px;">
        <hr />
        <dl class="row">
            <dt class="col-sm-2 uplabel">
                @Html.DisplayNameFor(model => model.TeamName)
            </dt>
            <dd class="col-sm-10" style="color: white;">
                : @Html.DisplayFor(model => model.TeamName)
            </dd>
            <dt class="col-sm-2 uplabel">
                @Html.DisplayNameFor(model => model.Member1)
            </dt>
            <dd class="col-sm-10" style="color: white;">
                : @Html.DisplayFor(model => model.Member1)
            </dd>
            <dt class="col-sm-2 uplabel">
                @Html.DisplayNameFor(model => model.Member2)
            </dt>
            <dd class="col-sm-10" style="color: white;">
                : @Html.DisplayFor(model => model.Member2)
            </dd>
            <dt class="col-sm-2 uplabel">
                @Html.DisplayNameFor(model => model.Member3)
            </dt>
            <dd class="col-sm-10" style="color: white;">
                : @Html.DisplayFor(model => model.Member3)
            </dd>
            <dt class="col-sm-2 uplabel">
                @Html.DisplayNameFor(model => model.Email)
            </dt>
            <dd class="col-sm-10" style="color: white;">
                : @Html.DisplayFor(model => model.Email)
            </dd>
            <dt class="col-sm-2 uplabel">
                @Html.DisplayNameFor(model => model.Phonenumber)
            </dt>
            <dd class="col-sm-10" style="color: white;">
                : @Html.DisplayFor(model => model.Phonenumber)
            </dd>

        </dl>

        <form asp-action="TeamDelete">
            <input type="hidden" asp-for="TId" />
            <input type="submit" value="Delete" class="btn2 btn2up" /> 
        </form>
    </div>
</div>
