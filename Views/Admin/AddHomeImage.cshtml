@model MyWebApp.Models.ImageUp

@{
    ViewData["Title"] = "Image Add";
}

<div style="align-items: center;
  justify-content: left;">
    <h1 class="textstylei">Add Home Image</h1>
</div>
<hr />
<div class="">
    <div class="row" style="margin-left: 80px;margin-top=20px;">
        <div class="col-md-4">
            <form enctype="multipart/form-data" method="post" asp-action="AddHomeImage">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                < <div class="form-group">
                    <p class="uplabel">Upload Picture </p>
                    <input name="files" type="file" class="form-control" />
        </div>

        <div class="form-group">
            <input type="submit" value="Save" class="btn2 btn2up" />
        </div>
        </form>
    </div>
</div>


    @section Scripts {
@{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
