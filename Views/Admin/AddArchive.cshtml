@model MyWebApp.Models.Archive

@{
    ViewData["Title"] = "Add Teacher";
}

<h1>Add Archive</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="AddArchive">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            
             <div class="form-group">
                <label asp-for="ContestName" class="control-label"></label>
                <input asp-for="ContestName" class="form-control" />
                <span asp-validation-for="ContestName" class="text-danger"></span>
            </div>
            
             <div class="form-group">
                <label asp-for="Year" class="control-label"></label>
                <input asp-for="Year" class="form-control" />
                <span asp-validation-for="Year" class="text-danger"></span>
            </div>
            
             <div class="form-group">
                <label asp-for="Position" class="control-label"></label>
                <input asp-for="Position" class="form-control" />
                <span asp-validation-for="Position" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TeamName" class="control-label"></label>
                <input asp-for="TeamName" class="form-control" />
                <span asp-validation-for="TeamName" class="text-danger"></span>
            </div>
            
             <div class="form-group">
                <label asp-for="Member1" class="control-label"></label>
                <input asp-for="Member1" class="form-control" />
                <span asp-validation-for="Member1" class="text-danger"></span>
            </div>
            
             <div class="form-group">
                <label asp-for="Member2" class="control-label"></label>
                <input asp-for="Member2" class="form-control" />
                <span asp-validation-for="Member2" class="text-danger"></span>
            </div>
            
             <div class="form-group">
                <label asp-for="Member3" class="control-label"></label>
                <input asp-for="Member3" class="form-control" />
                <span asp-validation-for="Member3" class="text-danger"></span>
            </div>
            
            
            <div class="form-group">
                <input type="submit" value="AddArchive" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

