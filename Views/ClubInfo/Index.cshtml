@model MyWebApp.Models.CollectionDataModel


<div>
    <h4 style="text-align:center">Teacher info</h4>
    @if (Model.Teachers != null)
    {

        <table>
            <tr>
                <th style="padding: 15px">Id</th>
                <th style="padding: 15px">Name</th>
                <th style="padding: 15px">Email</th>
            </tr>
            @foreach (Teacher teacher in Model.Teachers)
            {
                <tr>
                    <td>@teacher.Id</td>
                    <td>@teacher.FullName</td>
                    <td>@teacher.Email</td>
                    
                </tr>
            }
        </table>

    }
    else
    {
        <p>No Teacher is added</p>
    }
</div>






<div>
      <h4  style="text-align:center">Member info</h4>
@if(Model.usersWithRating !=null)
{
 
    <table >
    <tr>
        <th  style="padding: 15px"  >Name</th>
        <th  style="padding: 15px"  >Codeforces Id</th>
        <th  style="padding: 15px" >Cf Rating</th>
        <th  style="padding: 15px" >Email</th>
    </tr>
    @foreach (UserWithRating u in Model.usersWithRating)
    {
        <tr>
            <td>@u.users.FullName</td>
            <td>@u.users.CfId</td>
            <td>@u.rating</td>
            <td>@u.users.Email</td>
        </tr> 
    }
</table>

}
else
{
    <p>No User is added</p>
}
</div>



<div>
    <h4 style="text-align:center">Team info</h4>
    @if (Model.Teams != null)
    {

        <table>
            <tr>
                <th style="padding: 15px">Team Name</th>
                <th style="padding: 15px">Member1</th>
                <th style="padding: 15px">Member2</th>
                <th style="padding: 15px">Member3</th>
            </tr>
            @foreach (Team t in Model.Teams)
            {
                <tr>
                    <td>@t.TeamName</td>
                    <td>@t.Member1</td>
                    <td>@t.Member2</td>
                    <td>@t.Member3</td>
                </tr>
            }
        </table>

    }
    else
    {
        <p>No Team is added</p>
    }
</div>




<div>
    <h4 style="text-align:center">Archives</h4>
    @if (Model.Archives != null)
    {

        <table>
            <tr>
                <th style="padding: 15px">Year</th>
                <th style="padding: 15px">Position</th>
                <th style="padding: 15px">Contest Name</th>
                <th style="padding: 15px">Team Name</th>
                <th style="padding: 15px">Members</th>
            </tr>
            @foreach (Archive a in Model.Archives)
            {
                <tr>
                    <td>@a.Year</td>
                    <td>@a.Position</td>
                    <td>@a.ContestName</td>
                    <td>@a.TeamName</td>
                    <td>
                        @a.Member1 <br/>
                        @a.Member2 <br/>
                        @a.Member3
                    </td>
                </tr>
            }
        </table>

    }
    else
    {
        <p>No archives is added</p>
    }
</div>

