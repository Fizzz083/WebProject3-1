@model MyWebApp.Models.Notice

@{
    ViewData["Title"] = "Notice Delete";
}


<h1 class="textstylei">Notice Delete</h1>

<div style="margin-top: 40px;">
    <h3 style="font-weight:bold;color: rgb(105, 152, 240);">Are you sure you want to delete this?</h3>
    <div style="margin-top: 30px;">
        <hr />
        <dl class="row">
           <dt class="col-sm-2 uplabel">
                @Html.DisplayNameFor(model => model.ShortDescription)
            </dt>
            <dd class="col-sm-10 info">
                : @Html.DisplayFor(model => model.ShortDescription)
            </dd>
          <dt class="col-sm-2 uplabel">
                @Html.DisplayNameFor(model => model.Time)
            </dt>
            <dd class="col-sm-10 info">
                : @Html.DisplayFor(model => model.Time)
            </dd>
            <dt class="col-sm-2 uplabel">
                @Html.DisplayNameFor(model => model.Description)
            </dt>
            <dd class="col-sm-10 info">
                : @Html.DisplayFor(model => model.Description)
            </dd>


        </dl>
        <form enctype="multipart/form-data" method="post" asp-action="NoticeDelete">

            <!-- form enctype="multipart/form-data" method ="post" asp-action="TeacherDelete" -->
            <input type="hidden" asp-for="NId" />
            <input type="submit" value="Delete" class="btn2 btn2up" />
        </form>
    </div>
</div>


@section Scripts {
@{
    await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
